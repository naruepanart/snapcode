{
  "name": "snapcode",
  "displayName": "Snapcode Clean",
  "description": "Snapcode Clean for your code",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/naruepanart/snapcodeclean.git"
  },
  "publisher": "moyu",
  "keywords": [
    "snapcode clean",
    "polacode",
    "polaroid",
    "screenshot",
    "snippet",
    "share"
  ],
  "galleryBanner": {
    "color": "#fbfbfb",
    "theme": "light"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onCommand:snapcode.activate",
    "onWebviewPanel:snapcode"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "snapcode.activate",
        "title": "Snapcode Clean"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^11.12.0",
    "@types/vscode": "^1.32.0"
  }
}
